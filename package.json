{
  "name": "type-checking-MDX-in-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^5.3.11"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:mdx-loader": "tsc -p .storybook/webpack/tsconfig.json",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}' --format codeframe --color",
    "lint-fix": "yarn format && yarn lint",
    "format": "prettier -w './**/*.{js,ts,tsx}'",
    "sb": "yarn build:mdx-loader && start-storybook -p 9009 -s public",
    "sb:build": "yarn build:mdx-loader && build-storybook -s public",
    "postinstall": "patch-package"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-storyshots": "^6.5.16",
    "@storybook/addon-storyshots-puppeteer": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/cli": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@types/react-dom": "^18.3.1",
    "@types/styled-components": "^5.1.34",
    "eslint-config-prettier": "^8.10.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "import-sort-style-module": "^6.0.0",
    "patch-package": "^6.5.1",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-import-sort": "^0.0.7",
    "puppeteer": "^13.7.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "^4.9.5",
    "webpack": "5"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "resolutions": {
    "webpack-hot-middleware": "lukeapage/webpack-hot-middleware#master"
  }
}
