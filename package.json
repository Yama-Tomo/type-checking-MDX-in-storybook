{
  "name": "type-checking-MDX-in-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.3",
    "styled-components": "^5.2.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:mdx-loader": "tsc -p tsconfig.mdx-to-tsx-loader.json",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}' --format codeframe --color",
    "sb": "yarn build:mdx-loader && start-storybook -p 9009 -s public",
    "sb:build": "yarn build:mdx-loader && build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-storyshots": "^6.0.26",
    "@storybook/addon-storyshots-puppeteer": "^6.0.26",
    "@storybook/addons": "^6.0.26",
    "@storybook/cli": "^6.0.26",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.26",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.4",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.2",
    "puppeteer": "^5.3.1",
    "react-app-rewired": "^2.1.6",
    "react-refresh": "^0.8.3",
    "react-test-renderer": "^16.14.0",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  }
}
