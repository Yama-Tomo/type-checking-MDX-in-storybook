{
  "name": "type-checking-MDX-in-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:mdx-loader": "tsc -p tsconfig.mdx-to-tsx-loader.json",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}' --format codeframe --color",
    "sb": "yarn build:mdx-loader && start-storybook -p 9009 -s public",
    "sb:build": "yarn build:mdx-loader && build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@storybook/addon-docs": "next",
    "@storybook/addon-storyshots": "next",
    "@storybook/addon-storyshots-puppeteer": "next",
    "@storybook/addons": "next",
    "@storybook/cli": "next",
    "@storybook/preset-create-react-app": "^2.1.0",
    "@storybook/react": "next",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "@types/react": "16.9.25",
    "@types/react-dom": "16.9.5",
    "@types/styled-components": "^5.0.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^3.0.0",
    "prettier": "^2.0.2",
    "puppeteer": "^2.1.1",
    "react-app-rewired": "2.1.5",
    "react-hot-loader": "^4.12.20",
    "react-test-renderer": "^16.13.1",
    "typescript": "3.8.3"
  }
}